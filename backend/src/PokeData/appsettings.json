{
  "AllowedHosts": "*",
  "Caching": {
    "ActorLifetimeSeconds": 300,
    "LoadResourcesOnStartup": false,
    "ResourceLifetimeSeconds": 300
  },
  "Cors": {
    "AllowedOrigins": [],
    "AllowedMethods": [ "GET", "POST", "PUT" ],
    "AllowedHeaders": [ "Content-Type" ],
    "AllowCredentials": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
